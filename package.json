{
  "name": "ngx-cream-portal",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:fr": "ng serve --configuration fr",
    "build": "ng build",
    "build:GitHubPages": "ng build --base-href /ngx-cream-lib/ && cp dist/ngx-cream-portal/index.html dist/ngx-cream-portal/404.html",
    "build:lib": "ng build ngx-cream-lib",
    "publish:lib": "cd dist/ngx-cream-lib && npm publish",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:lib": "ng test --project ngx-cream-lib",
    "test:ci": "ng test --browsers=ChromeHeadless --watch=false --code-coverage",
    "lint": "ng lint --fix",
    "lint:check": "ng lint",
    "format": "prettier --config .prettierrc --write .",
    "format:check": "prettier --check .",
    "extract-i18": "ng extract-i18n --output-path src/locale"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.9",
    "@angular/cdk": "^17.3.9",
    "@angular/common": "^17.3.9",
    "@angular/compiler": "^17.3.9",
    "@angular/core": "^17.3.9",
    "@angular/forms": "^17.3.9",
    "@angular/platform-browser": "^17.3.9",
    "@angular/platform-browser-dynamic": "^17.3.9",
    "@angular/router": "^17.3.9",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "~0.14.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.7",
    "@angular-eslint/builder": "17.4.1",
    "@angular-eslint/eslint-plugin": "17.4.1",
    "@angular-eslint/eslint-plugin-template": "17.4.1",
    "@angular-eslint/schematics": "17.4.1",
    "@angular-eslint/template-parser": "17.4.1",
    "@angular/cli": "~17.3.7",
    "@angular/compiler-cli": "^17.3.9",
    "@angular/localize": "^17.3.9",
    "@types/jasmine": "~5.1.4",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jasmine-core": "~5.1.2",
    "karma": "~6.4.3",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^17.3.0",
    "prettier": "3.2.5",
    "prettier-eslint": "^16.3.0",
    "typescript": "~5.4.5"
  }
}
